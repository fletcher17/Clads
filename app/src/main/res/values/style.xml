<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SignUpScreenCreateAccount">
        <item name="android:layout_marginStart">@dimen/margin_18dp</item>
        <item name="android:layout_marginTop">@dimen/margin_75dp</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:text">@string/create_an_account</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textSize">@dimen/text_size_18sp</item>
    </style>

    <style name="SignUpScreenGetToKnowYouText">
        <item name="android:layout_marginStart">@dimen/margin_18dp</item>
        <item name="android:layout_marginTop">@dimen/margin_9dp</item>
        <item name="fontFamily">@font/poppins</item>
        <item name="android:text">@string/lets_get_to_know_you</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textSize">@dimen/text_size_14sp</item>
    </style>

    <style name="SignUpScreenEditTextField">
        <item name="fontFamily">@font/roboto</item>
        <item name="android:inputType">textPersonName</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textColorHint">@color/off_white</item>
        <item name="strokeColor">@color/gray</item>
        <item name="android:textCursorDrawable">@drawable/cursor_appearance</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
    </style>

    <style name="SignUpScreenSignUpButton">
        <item name="android:layout_marginTop">@dimen/margin_40dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_18dp</item>
        <item name="backgroundTint">@color/off_white</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:padding">@dimen/padding_10</item>
        <item name="android:text">@string/sign_up</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/deep_sky_blue</item>
        <item name="cornerRadius">@dimen/corner_radius_12dp</item>
    </style>

    <style name="CladSignUpScreenHaveAccountText">
        <item name="fontFamily">@font/poppins</item>
        <item name="android:text">@string/have_an_account</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textSize">@dimen/text_size_14sp</item>
    </style>

    <style name="CladsEditTextLayoutStyle">
        <item name="android:layout_marginStart">@dimen/margin_18dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_18dp</item>
        <item name="hintTextAppearance">@style/TextAppearance.AppCompat.Small</item>
        <item name="android:layout_marginTop">@dimen/margin_25dp</item>
        <item name="android:textColorHint">@color/off_white</item>
        <item name="boxBackgroundColor">@color/deep_sky_blue</item>
        <item name="boxBackgroundMode">filled</item>
        <item name="boxStrokeColor">@color/gray</item>
        <item name="hintTextColor">@color/gray</item>
        <item name="errorEnabled">true</item>
        <item name="errorTextColor">@color/red</item>
    </style>

    <style name="CladEmailSignUpSpinner">
        <item name="android:layout_weight">1</item>
        <item name="background">@color/deep_sky_blue</item>
        <item name="android:inputType">none</item>
        <item name="android:textColorHint">@color/off_white</item>
        <item name="android:textCursorDrawable">@drawable/cursor_appearance</item>
        <item name="elevation">@dimen/elevation_5dp</item>
        <item name="paddingStart">@dimen/padding_0dp</item>
        <item name="paddingEnd">@dimen/padding_0dp</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
    </style>

    <style name="CladsSignUpLoginText">
        <item name="android:layout_marginStart">@dimen/margin_10dp</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:text">@string/login</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textSize">@dimen/text_size_14sp</item>

    </style>

    <style name="TextAppearanceLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/roboto</item>
        <item name="android:textSize">@dimen/text_size_20sp</item>
    </style>

    <style name="Dropdown" parent="Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu">
        <item name="boxBackgroundColor">@color/deep_sky_blue</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="strokeColor">@color/gray</item>
        <item name="boxBackgroundMode">filled</item>
        <item name="boxStrokeColor">@color/gray</item>
        <item name="boxStrokeErrorColor">@color/red</item>
        <item name="endIconDrawable">@drawable/ic_spinner_arrow_down_24</item>
        <item name="endIconTint">@color/off_white</item>
        <item name="errorEnabled">true</item>
        <item name="errorTextColor">@color/red</item>
        <item name="hintEnabled">false</item>
    </style>

    <style name="fragment_profile_page_payment_terms_headings">
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textColor">@color/navy_blue</item>
        <item name="android:textSize">@dimen/text_size_18sp</item>
    </style>

    <style name="edit_profile_page_dialog_sub_headings">
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:textColor">@color/deep_sky_blue</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
    </style>

    <style name="edit_profile_page_dialog_submit_buttons">
        <item name="android:textSize">@dimen/text_size_13sp</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textColor">@color/deep_sky_blue</item>
    </style>

    <style name="fragment_profile_edit_profile_page_payment_options_check_boxes">
        <item name="buttonTint">@color/deep_sky_blue</item>
        <item name="android:textColor">@color/menu_text_color</item>
        <item name="android:paddingStart">@dimen/margin_16dp</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
        <item name="android:fontFamily">@font/poppins</item>
    </style>

    <style name="add_client_fragment_add_client_text_View">
        <item name="android:layout_marginTop">@dimen/dimen_44_dp</item>
        <item name="android:layout_marginLeft">@dimen/margin_27dp</item>
        <item name="android:layout_marginBottom">@dimen/margin_16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">top</item>
        <item name="android:textSize">@dimen/text_size_20_sp</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:textColor">@color/navy_blue</item>
    </style>

    <style name="add_client_account_gender_style" parent="input_text_style">
        <item name="android:layout_marginStart">@dimen/margin_15dp</item>
        <item name="android:layout_marginBottom">@dimen/dimen_16_dp</item>
    </style>

    <style name="input_text_style">
        <item name="android:textAlignment">textEnd</item>
        <item name="strokeColor">@color/text_input_line_grey</item>
        <item name="prefixTextColor">@color/navy_blue</item>
        <item name="android:maxEms">15</item>
        <item name="android:textSize">@dimen/text_size_12sp</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:textColor">@color/menu_text_color</item>
    </style>

    <style name="radio_input_text_style">
        <item name="android:textAlignment">textEnd</item>
        <item name="strokeColor">@color/text_input_line_grey</item>
        <item name="prefixTextColor">@color/navy_blue</item>
        <item name="android:textSize">@dimen/text_size_12sp</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:textColor">#506992</item>
        <item name="android:buttonTint">@color/navy_blue</item>
    </style>

    <style name="prefix_text_style">
        <item name="boxStrokeColor">@color/text_input_line_grey</item>
        <item name="prefixTextColor">@color/navy_blue</item>
        <item name="android:textSize">@dimen/text_size_14sp</item>
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:textColor">@color/card_text_color</item>
    </style>

    <style name="add_client_fragment_next_button">
        <item name="textAllCaps">false</item>
        <item name="backgroundTint">@color/blue</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginEnd">@dimen/margin_24dp</item>
        <item name="android:layout_marginStart">@dimen/margin_24dp</item>
        <item name="android:layout_marginBottom">@dimen/margin_10dp</item>
    </style>

    <style name="text_input_layout">
        <item name="expandedHintEnabled">false</item>
        <item name="android:layout_marginStart">@dimen/margin_10dp</item>
        <item name="android:layout_marginEnd">@dimen/margin_10dp</item>
        <item name="boxBackgroundMode">none</item>
        <item name="boxStrokeColor">@color/text_input_line_grey</item>
        <item name="hintEnabled">false</item>
        <item name="prefixTextColor">@color/navy_blue</item>
        <item name="prefixTextAppearance">@style/prefix_text_style</item>
    </style>

    <style name="tab_layout_style">
        <item name="android:gravity">fill</item>
        <item name="paddingEnd">@dimen/padding_15_dp</item>
        <item name="tabPaddingStart">@dimen/padding_15_dp</item>
        <item name="tabTextAppearance">@style/TextAppearance.AppCompat.Medium</item>
    </style>

    <style name="radio_styles">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">@dimen/margin_top_28dp</item>
        <item name="android:layout_marginBottom">@dimen/margin_24dp</item>
        <item name="layout_constraintHorizontal_bias">0.89</item>
    </style>

    <style name="EditProfileTabLayoutTabText">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/poppins_semibold</item>
